{
    "name": "Discord MusicBot",
    "id": "sudhanplayzdcmusicbot",
    "packaging_format": 1,
    "description": {
        "en": "An advanced discord music bot, supports Spotify, Soundcloud, YouTube with Shuffling, Volume Control and Web Dashboard with Slash Commands support! ",
    },
    "version": "1.0~ynh1",
    "url": "https://github.com/SudhanPlayz/Discord-MusicBot,
    "upstream": {
        "license": "free",
        "website": "https://musicbot.darrenofficial.com/",
        "code": "https://github.com/SudhanPlayz/Discord-MusicBot"
    },
    "license": "free",
    "maintainer": {
        "name": "Jame",
        "email": "yunohost@jameuwu.com"
    },
    "requirements": {
        "yunohost": ">= 11.0.0"
    },
    "multi_instance": true,
    "services": [
        "npm",
        "nodejs"
        "openjdk-17-jdk"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/example",
                "default": "/example"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "Discord bot token",
                "type": "string"
            }
        ]
    }
}
